{{ header }}{{ column_left }} 
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <a href="{{ confirm }}" data-toggle="tooltip" title="{{ button_confirm }}" class="btn btn-primary">{{ button_confirm }}</a>
        <a href="{{ dispute }}" data-toggle="tooltip" title="{{ button_dispute }}" class="btn btn-info">{{ button_dispute }}</a>
        <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default">{{ button_cancel }}</a>
      </div>
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %} 
        <li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
        {% endfor %} 
      </ul>
    </div>
  </div>
    
    <div class="container-fluid">
      {% if (error_warning) %} 
        <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }} 
          <button type="button" class="close" data-dismiss="alert">&times;</button>
        </div>
        {% endif %} 
        {% if (success) %} 
        <div class="alert alert-success"><i class="fa fa-check-circle"></i> {{ success }} 
          <button type="button" class="close" data-dismiss="alert">&times;</button>
        </div>
        {% endif %} 

        <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ 'Fruugo Stock' }}</h3>
          </div>
            <div class="panel-body">
                <div class="content">
                    <h2> Return Details</h2>
                    <div class="table-responsive">
                    <table class="table table-bordered table-hover">
                        {% for index,value in return_data %} 
                        <tr><td>{{ index }}</td><td>{{ value }}</td></tr>
                        {% endfor %} 
                    </table>
                    </div>
                    <h2> Images </h2>
                    <div class="table-responsive">
                    <table class="table table-bordered table-hover">
                        {% for index,value in images %} 
                        <tr><td>{{ index }}</td><td>{{ value }}</td></tr>
                        {% endfor %} 
                    </table>
                    </div>
                    <h2> User Details </h2>
                    <div class="table-responsive">
                    <table class="table table-bordered table-hover">
                        {% for index,value in user %} 
                        <tr><td>{{ index }}</td><td>{{ value }}</td></tr>
                        {% endfor %} 
                    </table>
                    </div>
                    <h2> Item Details </h2>
                    <div class="table-responsive">
                    <table class="table table-bordered table-hover">
                        {% for index,value in item %} 
                        <tr><td>{{ index }}</td><td>{{ value }}</td></tr>
                        {% endfor %} 
                    </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{ footer }} 